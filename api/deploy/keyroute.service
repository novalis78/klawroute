[Unit]
Description=KeyRoute API - WireGuard tunnels as a service
After=network.target wg-quick@wg0.service

[Service]
Type=simple
User=root
WorkingDirectory=/opt/keyroute
ExecStart=/usr/bin/node dist/index.js
Restart=always
RestartSec=5
EnvironmentFile=/opt/keyroute/.env

[Install]
WantedBy=multi-user.target
